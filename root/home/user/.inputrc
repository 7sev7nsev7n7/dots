# Options

set colored-stats on
set keyseq-timeout 1
set editing-mode vi
set show-mode-in-prompt on
set vi-cmd-mode-string "\1\e[33;1m\2N\1\e[0m\2 "
set vi-ins-mode-string ""

# Keybinds

$if mode=vi
  # Keymaps for when in insert mode
  set keymap vi-insert
  "\C-l": clear-screen
  "\C-e": shell-expand-line

  # Keymaps for when in command mode
  set keymap vi-command
  "\C-e": shell-expand-line
$endif
